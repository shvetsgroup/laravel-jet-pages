{
    "name": "shvetsgroup/laravel-jet-pages",
    "description": "Pages package for Laravel. Fast as a jet plane. Provides markdown support, localization and static cache.",
    "keywords": ["pages", "content", "laravel", "php"],
    "homepage": "https://github.com/ShvetsGroup/laravel-jet-pages",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "laravel/framework": "^6.0",
        "league/commonmark": "dev-document-pre-parsed-event as 1.4.x-dev",
        "symfony/yaml": "^4.0",
        "mad-web/laravel-robots": "~1.4",
        "watson/sitemap": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "graham-campbell/testbench": "~5.3"
    },
    "repositories": [
      {
        "type": "git",
        "url": "https://github.com/RefactoringGuru/commonmark"
      }
    ],
    "autoload": {
        "psr-4": {
            "ShvetsGroup\\JetPages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShvetsGroup\\Tests\\JetPages\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShvetsGroup\\JetPages\\JetPagesServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
