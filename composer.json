{
    "name": "shvetsgroup/laravel-jet-pages",
    "description": "Pages package for Laravel. Fast as a jet plane. Provides markdown support, localization and static cache.",
    "keywords": ["pages", "content", "laravel", "php"],
    "homepage": "https://github.com/ShvetsGroup/laravel-jet-pages",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=5.6.0",
        "league/commonmark": "^0.17",
        "watson/sitemap": "2.0.*",
        "ellisthedev/laravel-5-robots": "0.1.*",
        "symfony/yaml": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "graham-campbell/testbench": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "ShvetsGroup\\JetPages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShvetsGroup\\Tests\\JetPages\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShvetsGroup\\JetPages\\JetPagesServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
