{
    "name": "shvetsgroup/laravel-jet-pages",
    "description": "Pages package for Laravel. Fast as a jet plane. Provides markdown support, localization and static cache.",
    "keywords": [
        "pages",
        "content",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/ShvetsGroup/laravel-jet-pages",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "laravel/framework": "^7.0 || ^8.0",
        "league/commonmark": "^1.5",
        "symfony/yaml": "^5.0",
        "mad-web/laravel-robots": "^2.1",
        "watson/sitemap": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0",
        "graham-campbell/testbench": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "ShvetsGroup\\JetPages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShvetsGroup\\Tests\\JetPages\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShvetsGroup\\JetPages\\JetPagesServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
