{
    "name": "Laravel jet pages",
    "description": "Pages package for Laravel. Fast as a jet plane. Provides markdown support, localization and static cache.",
    "keywords": ["pages", "content", "laravel", "php"],
    "homepage": "https://github.com/ShvetsGroup/laravel-jet-pages",
    "license": "MIT",
    "version": "1.0",
    "require": {
        "php": ">=5.5.0",
        "laravel/framework": "~5.2",
        "graham-campbell/markdown": "^6.0",
        "watson/sitemap": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "ShvetsGroup\\JetPages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShvetsGroup\\Tests\\JetPages\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
